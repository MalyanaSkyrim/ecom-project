meta {
  name: List Products
  type: http
  seq: 2
}

get {
  url: {{API_URL}}/v1/products?pageSize=10&pageIndex=0&sorting[0][id]=name&sorting[0][direction]=desc
  body: none
  auth: bearer
}

params:query {
  pageSize: 10
  pageIndex: 0
  sorting[0][id]: name
  sorting[0][direction]: desc
  ~isFeatured: true
  ~searchText: wireless
}

auth:bearer {
  token: {{API_KEY}}
}
